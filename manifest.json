{
    "name":"megumichan",
    "version":"1.1",
    "manifest_version":2,

    "description":"天使あるいは天才。JKLTで音が出ます。オプションでキャラ切り替え",

    "background":{
        "scripts":[
            "jquery-1.9.1.min.js",
            "background.js"
        ]
    },

    "options_page":"options.html",

    "permissions":[
        "storage"
    ],
    "content_scripts":[
        {
            "matches":["http://www.tumblr.com/dashboard*","https://www.tumblr.com/dashboard*"],
            "js":["jquery-1.9.1.min.js","jquery.ba-throttle-debounce.min.js","content.js"]
        }
    ]
}
